@article{ WOS:000204838500005,
Author = {Kung, Rebecca Lippmann and Kung, Peter and Linder, Cedric},
Title = {Equipment issues regarding the collection of video data for research},
Journal = {PHYSICAL REVIEW SPECIAL TOPICS-PHYSICS EDUCATION RESEARCH},
Year = {2005},
Volume = {1},
Number = {1},
Month = {JUL-DEC},
Abstract = {Physics education research increasingly makes use of video data for
   analysis of student learning and teaching practice. Collection of these
   data is conceptually simple but execution is often fraught with costly
   and time-consuming complications. This pragmatic paper discusses the
   development of systems to record and permanently archive audio and video
   data in real-time. We focus on a system based upon consumer video DVD
   recorders, but also give an overview of other technologies and detail
   issues common to all systems. We detail common yet unexpected
   complications, particularly with regard to sound quality and
   compatibility with transcription software. Information specific to fixed
   and transportable systems, other technology options, and generic and
   specific equipment recommendations are given in supplemental appendices},
