@article{ WOS:000642487500001,
Author = {Liao, Wei and Wang, Xuewei and Qin, Kongji},
Title = {Learning to become culturally responsive teacher educators in an
   internationalized doctoral course: A video-cued interpretive study},
Journal = {TEACHING AND TEACHER EDUCATION},
Year = {2021},
Volume = {102},
Month = {JUN},
Abstract = {Using a video-cued interpretive research methodology, this study
   examines how eight doctoral students learn to become culturally
   responsive teacher educators in a foundation course in one of the first
   international teacher education Ph.D. programs in mainland China. We
   identified three patterns under girding the participants' learning
   experiences: (a) engaging with teacher education knowledge from
   culturally plural perspectives; (b) honing strategies for creating
   culturally inclusive learning experiences; and (c) developing
   asset-based views toward cultural diversity. These patterns constitute a
   conceptualization of the process of learning to become culturally
   responsive teacher educators and suggest practical implications for
   preparing culturally responsive teacher educators.
