@article{Haven.etal.2020,
  title = {Preregistering {{Qualitative Research}}: {{A Delphi Study}}},
  shorttitle = {Preregistering {{Qualitative Research}}},
  author = {Haven, Tamarinde L. and Errington, Timothy M. and Gleditsch, Kristian Skrede and {van Grootel}, Leonie and Jacobs, Alan M. and Kern, Florian G. and Pi{\~n}eiro, Rafael and Rosenblatt, Fernando and Mokkink, Lidwine B.},
  year = {2020},
