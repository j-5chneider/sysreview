  author = {Selting, Margret and Auer, Peter and {Barth-Weingarten}, Dagmar and Bergmann, J{\"o}rg R. and Bergmann, Pia and Birkner, Karin and {Couper-Kuhlen}, Elizabeth and Deppermann, Arnulf and Gilles, Peter and G{\"u}nthner, Susanne and Hartung, Martin and Kern, Friederike and Mertzlufft, Christine and Meyer, Christian and Morek, Miriam and Oberzaucher, Frank and Peters, J{\"o}rg and Quasthoff, Uta and Sch{\"u}tte, Wilfried and Stukenbrock, Anja and Uhmann, Susanne},
