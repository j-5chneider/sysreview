@article{Tan.etal.2025,
  title = {Development and Validation of {{Join Clinical Trial Questionnaire}} ({{JoinCT}})},
  author = {Tan, Shirin Hui and King, Teck Long and Tan, Shirley Siang Ning and Lai, Wei Hong and Bujang, Mohamad Adam and Voon, Pei Jye},
  year = {2025},
