@article{Bottcher.Thiel.2018,
  title = {Evaluating Research-Oriented Teaching: A New Instrument to Assess University Students' Research Competences},
  shorttitle = {Evaluating Research-Oriented Teaching},
  author = {B{\"o}ttcher, Franziska and Thiel, Felicitas},
  year = {2018},
  month = jan,
  journal = {Higher Education},
  volume = {75},
