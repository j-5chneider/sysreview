@article{Scherhag.Burgard.2023,
  title = {Performance of {{Semi-Automated Screening Using Rayyan}} and {{ASReview}}: {{A Retrospective Analysis}} of {{Potential Work Reduction}} and {{Different Stopping Rules}}},
