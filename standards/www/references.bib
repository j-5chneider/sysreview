@article{Campbell.etal.2023a,
  title = {Understanding {{Sexual Assault Survivors}}' {{Perspectives}} on {{Archiving Qualitative Data}}: {{Implications}} for {{Feminist Approaches}} to {{Open Science}}},
  shorttitle = {Understanding {{Sexual Assault Survivors}}' {{Perspectives}} on {{Archiving Qualitative Data}}},
  author = {Campbell, Rebecca and {Goodman-Williams}, Rachael and Javorka, McKenzie and Engleton, Jasmine and Gregory, Katie},
