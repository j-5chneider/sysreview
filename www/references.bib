@article{Jacobs.etal.2021,
  title = {The {{Qualitative Transparency Deliberations}}: {{Insights}} and {{Implications}}},
  shorttitle = {The {{Qualitative Transparency Deliberations}}},
  author = {Jacobs, Alan M. and B{\"u}the, Tim and Arjona, Ana and Arriola, Leonardo R. and Bellin, Eva and Bennett, Andrew and Bj{\"o}rkman, Lisa and Bleich, Erik and Elkins, Zachary and Fairfield, Tasha and Gaikwad, Nikhar and Greitens, Sheena Chestnut and Hawkesworth, Mary and Herrera, Veronica and Herrera, Yoshiko M. and Johnson, Kimberley S. and Karako{\c c}, Ekrem and Koivu, Kendra and Kreuzer, Marcus and Lake, Milli and Luke, Timothy W. and MacLean, Lauren M. and Majic, Samantha and Maxwell, Rahsaan and Mampilly, Zachariah and Mickey, Robert and Morgan, Kimberly J. and Parkinson, Sarah E. and Parsons, Craig and Pearlman, Wendy and Pollack, Mark A. and Posner, Elliot and Riedl, Rachel Beatty and Schatz, Edward and Schneider, Carsten Q. and Schwedler, Jillian and Shesterinina, Anastasia and Simmons, Erica S. and Singerman, Diane and Soifer, Hillel David and Smith, Nicholas Rush and Spitzer, Scott and Tallberg, Jonas and Thomson, Susan and {V{\'a}zquez-Arroyo}, Antonio Y. and Vis, Barbara and Wedeen, Lisa and Williams, Juliet A. and Wood, Elisabeth Jean and Yashar, Deborah J.},
  year = {2021},
  month = mar,
  journal = {Perspectives on Politics},
  volume = {19},
  number = {1},
  pages = {171--208},
  issn = {1537-5927, 1541-0986},
  doi = {10.1017/S1537592720001164},
  urldate = {2022-11-29},
  abstract = {In recent years, a variety of efforts have been made in political science to enable, encourage, or require scholars to be more open and explicit about the bases of their empirical claims and, in turn, make those claims more readily evaluable by others. While qualitative scholars have long taken an interest in making their research open, reflexive, and systematic, the recent push for overarching transparency norms and requirements has provoked serious concern within qualitative research communities and raised fundamental questions about the meaning, value, costs, and intellectual relevance of transparency for qualitative inquiry. In this                Perspectives                Reflection, we crystallize the central findings of a three-year deliberative process\textemdash the Qualitative Transparency Deliberations (QTD)\textemdash involving hundreds of political scientists in a broad discussion of these issues. Following an overview of the process and the key insights that emerged, we present summaries of the QTD Working Groups' final reports. Drawing on a series of public, online conversations that unfolded at                www.qualtd.net                , the reports unpack transparency's promise, practicalities, risks, and limitations in relation to different qualitative methodologies, forms of evidence, and research contexts. Taken as a whole, these reports\textemdash the full versions of which can be found in the Supplementary Materials\textemdash offer practical guidance to scholars designing and implementing qualitative research, and to editors, reviewers, and funders seeking to develop criteria of evaluation that are appropriate\textemdash as understood by relevant research communities\textemdash to the forms of inquiry being assessed. We dedicate this Reflection to the memory of our coauthor and QTD working group leader Kendra Koivu.                1},
  langid = {english},
  keywords = {paper: A-N-Modell,paper: Prozessmodell},
  file = {C:\Users\schneider3\Zotero\storage\D2DME2MI\jacobs2021.pdf}
}

