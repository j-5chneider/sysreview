@article{Schrader.etal.2020,
  title = {{Stichwortbeitrag Implementationsforschung: Wie Wissenschaft zu Verbesserungen im Bildungssystem beitragen kann}},
  shorttitle = {{Stichwortbeitrag Implementationsforschung}},
  author = {Schrader, Josef and Hasselhorn, Marcus and Hetfleisch, Petra and Goeze, Annika},
  year = {2020},
