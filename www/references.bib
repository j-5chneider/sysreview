@article{Agley.etal.2022,
  title = {Quality Control Questions on {{Amazon}}'s {{Mechanical Turk}} ({{MTurk}}): {{A}} Randomized Trial of Impact on the {{USAUDIT}}, {{PHQ-9}}, and {{GAD-7}}},
  shorttitle = {Quality Control Questions on {{Amazon}}'s {{Mechanical Turk}} ({{MTurk}})},
  author = {Agley, Jon and Xiao, Yunyu and Nolan, Rachael and {Golzarri-Arroyo}, Lilian},
  year = {2022},
