@article{Schmid.etal.2020,
  title = {Developing a Short Assessment Instrument for {{Technological Pedagogical Content Knowledge}} ({{TPACK}}.Xs) and Comparing the Factor Structure of an Integrative and a Transformative Model},
  author = {Schmid, Mirjam and Brianza, Eliana and Petko, Dominik},
  year = {2020},
